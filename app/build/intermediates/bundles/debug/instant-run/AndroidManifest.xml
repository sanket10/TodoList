<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sanket.todolist"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="24" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="TodoList"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.sanket.todolist.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.sanket.todolist.ShowDetail"
            android:parentActivityName="com.example.sanket.todolist.MainActivity" />
        <activity
            android:name="com.example.sanket.todolist.AddItem"
            android:parentActivityName="com.example.sanket.todolist.MainActivity" />
    </application>

</manifest>